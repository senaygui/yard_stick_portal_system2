<div class="row">
  <div class="col-12 col-md-6 form-group">
    <%= f.input :study_level, as: :select, :collection => ["undergraduate", "graduate", "TPVT"], input_html: {class: "form-control custom-select"}, :include_blank => false%>
  </div>
  <div class="col-12 col-md-6 form-group">
    <%= f.input :admission_type, as: :select, :collection => ["online", "regular", "extention", "distance"], input_html: {class: "form-control custom-select"}, :include_blank => false%>
  </div>
  <div class="col-12 col-md-6 form-group">
    <%= f.input :program_id, as: :select, :collection => Program.all.pluck(:program_name, :id), input_html: {class: "form-control custom-select"}%>
  </div>
</div>